@using EntitiyLayer.Concrete
@model List<About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Hakkında veri listesi</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>HAKKIMDA BAŞLIĞI</th>
        <th>AKTİF YAP</th>
        <th>PASİF YAP</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.AboutId</td>
            <td>Başlık</td>
            <td><a href="#" class="btn btn-warning">Aktif Yap</a></td>
            <td><a href="#" class="btn btn-danger">Pasif Yap</a></td>

        </tr>
    }
</table>
@Html.Action("AboutPartial", "About")